-- update duck funtions

function update_move_duck()
	if btn(⬅️) then
		duck.x-=1
	end
	if btn(➡️) then
		duck.x+=1
	end
	if btn(⬆️) then
		duck.y-=1
	end
	if btn(⬇️) then
			duck.y+=1
	end
	if btn(⬅️) or btn(➡️) or btn(⬆️) or  btn(⬇️)then
		duck.move=1
	else
		duck.move=0
	end
	
	if btn(🅾️) then
		game.egg+=1
	end
	
	if game.egg > 20 then
			music(0)
	 	game.egg=0
	end
	
end

function update_limit_duck()
	if duck.x > 124 then
		duck.x=-4
	end
	if duck.x < -4 then
		duck.x=124
	end
	if duck.y > 124 then
		duck.y=10
	end
	if duck.y < 10 then
		duck.y=124
	end
end


function update_reset_duck()
	duck.x=63
	duck.y=63
	duck.sprite=1
	duck.flipx=false
	duck.move=0
	game.egg=0
end
