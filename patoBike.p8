function _init()
	print_game()
	music(4)
end

function _update()
		global_clock()
	if game.status == "menu" then
		update_menu()
	end
	
	if game.status == "game" then
		update_move_duck()
		update_limit_duck()
		update_bread_colision()
		draw_bread_animate()
	end
	
	if game.status == "finish" then
			update_end_game()
	end
end

function _draw()
	palt(0,false)
	palt(8,true)
	
	if game.status=="menu" then
		draw_menu()
	end
	
	if game.status == "game" then
		draw_game()
	
	end
	
	if game.status == "finish" then
			draw_end_game()

	end
	
end

function draw_game()
		cls(4)
		print_game()
		draw_duck()
		draw_duck_sprite()
		draw_bread()
		draw_bread_colision()
		print_score()
		print_gc()
end
